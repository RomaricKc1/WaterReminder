    <!-- Notifs-->
    <string name="notif_channel_name">Rappel hydratation</string>
    <string name="notif_text1">Buvez de l’eau et sentez-vous rafraîchi !</string>
    <string name="notif_text2">"Restez hydraté et restez en bonne santé ! "</string>
    <string name="notif_text3">"L'eau, c'est la vie. Hydratez-vous !"</string>
    <string name="notif_text4">"Vous pouvez y arriver ! Reste hydraté."</string>
    <string name="notif_text5">Restez calme et buvez de l\'eau.</string>
    <string name="notif_text6">"Boire de l’eau c'est la clé du succès."</string>
    <string name="notif_text7">"Votre corps vous remercie de boire de l’eau."</string>
    <string name="notif_text8">"Restez hydraté et ressentez la différence."</string>
    <string name="notif_text9">"Bon travail pour rester hydraté !"</string>
    <string name="notif_text10">"Continuez à siroter ! Vous vous en sortez très bien."</string>
    <string name="notif_title">Il est l\'heure de boire de l\'eau!</string>

    <!-- Tips-->
    <string name="tip">Astuce</string>
    <string name="tip1_title">"Commencez votre journée avec un verre d'eau"</string>
    <string name="tip1_desc">Boire un verre d’eau dès le matin peut vous aider à démarrer votre hydratation pour la journée.</string>
    <string name="tip2_title">"Emportez une bouteille d'eau avec vous"</string>
    <string name="tip2_desc">Avoir une bouteille d’eau avec vous à tout moment peut vous aider à vous rappeler de boire de l’eau tout au long de la journée.</string>
    <string name="tip3_title">"Buvez de l'eau avant et après l'exercice "</string>
    <string name="tip3_desc">"Rester hydraté avant et après l’exercice peut vous aider à être au meilleur de votre forme et à réduire le risque de déshydratation."</string>
    <string name="tip4_title">"Mangez des aliments riches en eau"</string>
    <string name="tip4_desc">"Des aliments comme la pastèque, les concombres et les fraises peuvent vous aider à rester hydraté et à ajouter de la variété à votre alimentation."</string>
    <string name="tip5_title">"Évitez les boissons sucrées"</string>
    <string name="tip5_desc">"Les boissons sucrées peuvent vous déshydrater et ajouter des calories inutiles à votre alimentation."</string>
    <string name="tip6_title">"Buvez de l'eau lorsque vous avez faim"</string>
    <string name="tip6_desc">"Parfois, la soif peut être confondue avec la faim, alors essayez de boire de l’eau avant de prendre une collation."</string>
    <string name="tip7_title">"Buvez de l'eau lorsque vous vous sentez fatigué"</string>
    <string name="tip7_desc">"La fatigue peut être un signe de déshydratation, alors essayez de boire de l’eau au lieu de consommer de la caféine ou du sucre."</string>
    <string name="tip8_title">"Aromatisez votre eau"</string>
    <string name="tip8_desc">"Ajouter des fruits, des herbes ou un peu de jus à votre eau peut la rendre plus attrayante et vous aider à boire davantage."</string>
    <string name="tip9_title">"Boire de l'eau avec les repas"</string>
    <string name="tip9_desc">"Boire de l’eau avec les repas peut aider à faciliter la digestion et vous garder hydraté tout au long de la journée."</string>
